#include<stdio.h>
#include<math.h>
int SoLe( int n[], int size){
	int count= 0;
	 for (int i = 0; i < size; i++) {
		if( n[i] % 2 != 0){
			count++;
		}
	}
	return count;
}
int main(){
	int a[]={1,4 ,-1, -3, -6,3};
	int size = sizeof(a)/sizeof(a[0]);
	int count = SoLe(a,size  );
	printf("Cac so le trong mang la: %d", count);
	return 0;
}
