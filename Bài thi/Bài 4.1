#include<stdio.h>
#include<math.h>
void SoDuong( int n[], int size){
	int count= 0;
	 for (int i = 0; i < size; i++) {
		if( n[i]>0){
			count++;
		}
	}
	printf("So luong so duong trong mang la: %d ", count);
}
int main(){
 int a[]={1,4 ,-1, -3, -6,3};
    int size = sizeof(a) / sizeof(a[0]);
    SoDuong(a,size);
    
}
