#include <stdio.h>

int main() {
    char grade;
   
    scanf(" %c", &grade);

    switch (grade) {
        case 'A':
            printf("Xuất sắc!");
            break;
        case 'B':
            printf("Tốt!");
            break;
        case 'C':
            printf("Khá!");
            break;
        case 'D':
            printf("Trung bình!");
            break;
        case 'F':
            printf("Rớt!");
            break;
        default:
            printf("Điểm không hợp lệ.");
    }

    return 0;
}
